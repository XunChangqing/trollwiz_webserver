<p id="notice"><%= notice %></p>

<h1>Listing Domain Update Logs</h1>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th>Domain name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @domain_update_logs.each do |domain_update_log| %>
      <tr>
        <td><%= domain_update_log.operation %></td>
        <td><%= domain_update_log.domain_name %></td>
        <td><%= link_to 'Show', domain_update_log %></td>
        <td><%= link_to 'Edit', edit_domain_update_log_path(domain_update_log) %></td>
        <td><%= link_to 'Destroy', domain_update_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Domain update log', new_domain_update_log_path %>
