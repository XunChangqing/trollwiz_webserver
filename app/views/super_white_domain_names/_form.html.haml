= form_for @super_white_domain_name do |f|
  - if @super_white_domain_name.errors.any?
    #error_explanation
      %h2= "#{pluralize(@super_white_domain_name.errors.count, "error")} prohibited this super_white_domain_name from being saved:"
      %ul
        - @super_white_domain_name.errors.full_messages.each do |msg|
          %li= msg

  .form-group
    = f.label :domain_name
    = f.text_field :domain_name
  .actions
    = f.submit 'Save'
